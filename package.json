    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@nx/js": "21.6.8",
    "@nx/esbuild": "21.6.8",
    "@nx/node": "21.6.8",
    "esbuild": "^0.24.0",
    "nx": "21.6.8",
    "typescript": "~5.8.2",
    "tslib": "^2.3.0"
  }
}

